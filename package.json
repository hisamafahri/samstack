{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "ios": "turbo run ios --filter=@samstack/mobile",
    "android": "turbo run android --filter=@samstack/mobile",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@samstack/eslint-config": "*",
    "eslint": "^8.39.0",
    "prettier": "^2.5.1",
    "turbo": "latest",
    "typescript": "^5.0.4"
  },
  "name": "samstack",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
